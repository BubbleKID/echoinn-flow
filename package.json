{
  "name": "client",
  "version": "1.0.0",
  "description": "front-end for the echoinn",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack serve --progress",
    "test": "jest --collect-coverage --updateSnapshot",
    "build": "webpack --progress",
    "lint": "npx link eslint & eslint ./src/**/*.{ts,tsx}",
    "gen": "graphql-codegen"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/index.tsx",
      "!src/setupTests.js",
      "!server.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy",
      "\\.(svg)$": "<rootDir>/src/_mock/svgrMock.js",
      "\\.(jpg|ico|jpeg|png)$": "<rootDir>/src/_mocks/fileMock.js"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
